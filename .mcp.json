{
  "data-analyst-db": {
    "command": "python",
    "args": ["${CLAUDE_PLUGIN_ROOT}/mcp-server/server.py"],
    "env": {
      "DATABASE_URL": "${DATABASE_URL}",
      "DB_URL": "${DB_URL}",
      "DB_TYPE": "${DB_TYPE}",
      "DB_HOST": "${DB_HOST}",
      "DB_PORT": "${DB_PORT}",
      "DB_NAME": "${DB_NAME}",
      "DB_USER": "${DB_USER}",
      "DB_PASSWORD": "${DB_PASSWORD}"
    }
  }
}
